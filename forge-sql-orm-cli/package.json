{
  "name": "forge-sql-orm-cli",
  "version": "2.0.25",
  "description": "CLI tool for Forge SQL ORM",
  "main": "dist-cli/cli.js",
  "types": "dist-cli/cli.d.ts",
  "bin": {
    "forge-sql-orm-cli": "./dist-cli/forgeSqlOrmCLI.js"
  },
  "scripts": {
    "lint": "eslint src  --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src",
    "clean": "rm -rf dist",
    "build": "npm run clean && vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "files": [
    "dist-cli",
    "tsconfig.json",
    "src",
    "README.md"
  ],
  "keywords": [
    "atlassian-forge",
    "atlassian-forge-drizzle",
    "forge-sql",
    "forge",
    "drizzle",
    "drizzle-orm",
    "drizzle-driver",
    "drizzle-custom-driver",
    "orm",
    "database",
    "sql",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0",
    "dotenv": "^17.0.0",
    "drizzle-kit": "^0.31.4",
    "forge-sql-orm": "^2.0.25",
    "inquirer": "^12.6.3",
    "mysql2": "^3.14.1",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.44.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  }
}
